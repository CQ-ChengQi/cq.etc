#FORM2

[活动时间]
# 任务开始时间，持续时间，开启频率
time_list$ : ({ "2023-08-09-00:00:00", "EVERY_DAY(100)", "EVERY_WEEK(0)", "2026-10-30-23:59:59" })

[broadcast_setting]
broadcast_list:   ({ "start_task", "end_task" })

[start_task]
broadcast_start : "start_time"
interval        : 7200
broadcast_end   : "end_time(-60)"
message         : "#P元宝商人#P许愿活动开始了，大家快来参与吧！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[end_task]
broadcast_start : "end_time"
interval        : 0
broadcast_end   : "end_time(+60)"
message         : "#P元宝商人#P许愿活动结束了，感谢大家的参与。"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

